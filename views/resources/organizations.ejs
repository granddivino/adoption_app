this is a page to display the search results for oragnizations

<div class="org-page">

    <% if (orgResults) { %> <% orgResults.forEach(org => { %>
        <div>
            <h3><a href="<%= org.url %>"><%= org.name %></a>
                <br>
                <%= org.email %><br>
                <%= org.phone %><br>
                <a href="<%= org.url %>">check out the website</a>
            </h3>
            <form method="POST" action="/organizations/kept">
                <input hidden type="text" name="orgId" value="<%= org.id %>">
                <input hidden type="text" name="name" value="<%= org.name %>">
                <input type="hidden" name="userId" value="<%= user.id %>">
                <button type="submit">save</button>
            </form>
            <hr>
        </div>
    <% }); %>
    <% } else { %>
        <div>nothing found</div>
    <% } %>
</div>
